{
  "name": "openapi-scorer",
  "version": "1.0.0",
  "description": "A comprehensive OpenAPI specification scoring and validation tool",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "openapi",
    "swagger",
    "validation",
    "scoring",
    "api"
  ],
  "author": "OpenAPI Scorer",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "js-yaml": "^4.1.0",
    "openapi-typescript": "^6.7.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
